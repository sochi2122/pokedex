          <h1>fooBar()</h1>
          <ul>
            <% for(let i = 0; i < pokemon.length; i++) { %>
              <li><a href="/drinks/<%=i%>"><%= drinks[i].name %></a></li>
            <% } %>
          </ul>
        </div>
        <div>
          <h1>fooD()</h1>
